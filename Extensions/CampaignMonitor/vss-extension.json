{
  "manifestVersion": 1,
  "id": "campaignmonitor",
  "version": "0.0.27",
  "name": "Campaign Monitor Tasks",
  "scopes": [],
  "description": "Various tasks for using Campaign Monitor",
  "publisher": "gregpakes",
  "galleryFlags": [
    "public"
  ],
  "icons": {
    "default": "static/images/campaignmonitor.jpg"
  },
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "demands": [
    "api-version/3.0"
  ],
  "tags": [
    "CampaignMonitor",
    "Campaign Monitor",
    "CreateSend",
    "Create Send"
  ],
  "categories": [
    "Build and release"
  ],
  "screenshots": [],
  "content": {
    "license": {
      "path": "license.md"
    },
    "details": {
      "path": "readme.md"
    }
  },
  "links": {
    "getstarted": {
      "uri": "https://github.com/gregpakes/vsts.extensions"
    },
    "support": {
      "uri": "https://github.com/gregpakes/vsts.extensions"
    },
    "issues": {
      "uri": "https://github.com/gregpakes/vsts.extensions"
    }
  },
  "repository": {
    "type": "git",
    "uri": "https://github.com/gregpakes/vsts.extensions"
  },
  "branding": {
    "color": "rgb(220, 235, 252)",
    "theme": "light"
  },
  "files": [
    {
      "path": "CreateCampaignTask"
    },
    {
      "path": "DeleteCampaignTask"
    }
  ],
  "contributions": [
    {
      "id": "CreateCampaignTask",
      "description": "Create a draft campaign in campaignmonitor",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "type": "ms.vss-distributed-task.task",
      "properties": {
        "name": "CreateCampaignTask"
      }
    },
    {
      "id": "DeleteCampaignTask",
      "description": "Delete a draft campaign in campaignmonitor",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "type": "ms.vss-distributed-task.task",
      "properties": {
        "name": "DeleteCampaignTask"
      }
    }
  ]
}